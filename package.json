{
  "name": "stranerd",
  "version": "1.0.0",
  "description": "welcome to the stranerd official page",
  "private": true,
  "scripts": {
    "dev": "npm run env:copy:app && nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint src/ --ext .ts,.js,.vue --fix && stylelint src/**/*.scss src/**/*.vue --fix",
    "firebase:start": "npm run firebase:functions:build && npm run env:copy:functions && firebase emulators:start --import ./firebase/docs/exports -P development",
    "firebase:save": "firebase emulators:export ./firebase/docs/exports -P development",
    "firebase:functions:watch": "npm run watch --prefix ./firebase/functions",
    "firebase:functions:build": "npm run build --prefix ./firebase/functions",
    "firebase:functions:lint": "npm run lint --prefix ./firebase/functions",
    "env:set:functions": "node bin/set-functions-config",
    "env:copy:example": "copy env.example.json env.json",
    "env:copy:functions": "node bin/copy-env-functions",
    "env:copy:app": "node bin/copy-env-app",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@nuxtjs/composition-api": "^0.24.6",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.2.0",
    "@stripe/stripe-js": "^1.15.1",
    "@vue-stripe/vue-stripe": "^4.2.5",
    "algoliasearch": "^4.10.3",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.2",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.15.2",
    "cors": "^2.8.5",
    "email-templates": "^8.0.7",
    "express": "^4.17.1",
    "firebase": "^8.7.0",
    "firebase-admin": "^9.10.0",
    "flutterwave-vue-v3": "^1.0.5",
    "nodemailer": "^6.6.2",
    "nuxt": "^2.15.7",
    "nuxt-compress": "^5.0.0",
    "sd-validate": "^1.0.4",
    "stripe": "^8.160.0",
    "sweetalert2": "^11.0.18",
    "vue": "^2.6.14",
    "vue-css-donut-chart": "^1.3.0",
    "vue-instantsearch": "^3.7.0",
    "vue-screen": "^1.5.4",
    "vue2-editor": "^2.10.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/email-templates": "^8.0.3",
    "@types/nodemailer": "^6.4.2",
    "@types/vue2-editor": "^2.6.1",
    "@typescript-eslint/parser": "^4.28.1",
    "babel-eslint": "^10.1.0",
    "child_process": "^1.0.2",
    "eslint": "^7.30.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "firebase-tools": "^9.14.0",
    "husky": "^7.0.0",
    "node-sass": "^5.0.0",
    "nuxt-purgecss": "^1.0.0",
    "sass-loader": "^10.2.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.19.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stranerd/stranerd.git"
  },
  "author": "Kevin Izuchukwu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stranerd/stranerd/issues"
  },
  "homepage": "https://github.com/stranerd/stranerd#readme"
}
