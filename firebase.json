{
	"firestore": {
	  "rules": "firebase/firestore.rules",
	  "indexes": "firebase/firestore.indexes.json"
	},
	"functions": {
	  "source": "firebase/functions",
	  "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build",
	  "runtime": "nodejs12"
	},
	"database": {
	  "rules": "firebase/database.rules.json"
	},
	"storage": {
	  "rules": "firebase/storage.rules"
	},
	"emulators": {
	  "functions": {
		"port": 5001
	  },
	  "firestore": {
		"port": 5002
	  },
	  "database": {
		"port": 5003
	  },
	  "auth": {
		  "port": 5004
	  },
		"storage": {
		  "port": 5005
		},
		"pubsub": {
		  "port": 5006
		 },
	  "ui": {
		"enabled": true,
		"port": 4000
	  }
	}
  }