{
	"rules": {
		"answers": {
			"$answerId": {
				"ratings": {
					"$userId": {
						".write": "auth.uid === $userId"
					}
				}
			}
		},
		"chats": {
			"meta": {
				"$userId": {
					".read": true,
					".write": "auth.uid !== null",
					".indexOn": [
						"last/dates/createdAt"
					]
				}
			},
			"single": {
				"$path": {
					".read": true,
					".write": "auth.uid != null",
					".indexOn": [
						"dates/createdAt"
					]
				}
			}
		},
		"comments": {
			"answers": {
				"$answerId": {
					".read": true,
					".write": "auth.uid != null"
				}
			},
			"questions": {
				"$questionId": {
					".read": true,
					".write": "auth.uid != null"
				}
			}
		},
		"forms": {
			"messages": {
				".write": true
			},
			"reports": {
				".write": "auth.uid != null"
			}
		},
		"profiles": {
			".read": true,
			".indexOn": [
				"roles/isAdmin",
				"bio/email",
				"account/rank",
				"tutor/strongestSubject"
			],
			"$userId": {
				".read": true,
				".write": "auth.uid === $userId",
				"status": {
					".write": true
				}
			}
		},
		"reports": {
			"$reportType": {
				".read": true,
				".indexOn": [
					"dates/createdAt"
				],
				"$reportId": {
					".read": true,
					".write": "auth.uid != null"
				}
			}
		},
		"subjects": {
			".read": true,
			"$id": {
				".read": true,
				".write": "auth.token.isAdmin === true"
			}
		},
		"tags": {
			".read": true,
			".indexOn": [
				"count"
			]
		},
		"users": {
			"$userId": {
				".read": true,
				".write": "auth.uid === $userId",
				"notifications": {
					".indexOn": [
						"dates/createdAt",
						"seen"
					]
				}
			}
		},
		".read": false,
		".write": false
	}
}
